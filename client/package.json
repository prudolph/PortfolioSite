{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.3",
    "firebase": "^4.11.0",
    "firebase-admin": "^5.10.0",
    "html-to-draftjs": "^1.3.0",
    "moment": "^2.22.0",
    "node-sass-chokidar": "^1.1.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-draft-wysiwyg": "^1.12.11",
    "react-modal": "^3.3.2",
    "react-redux": "^5.0.7",
