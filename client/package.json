{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "firebase": "^5.4.2",
    "firebase-admin": "^6.0.0",
    "html-to-draftjs": "^1.4.0",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.2",
